{
  "name": "fit-friends",
  "version": "1.0.0",
  "description": "FitFriends онлайн площадка для поиска тренировок и их создания.",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.js",
    "build": "npm run clean && npm run compile && npm run copy",
    "buildDockerImage": "npm run build && docker build --file Dockerfile --tag fit.friends:1.0 .",
    "lint": "eslint src/ --ext .ts",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "copy": "copy package.json dist\\ && copy README.md dist\\ && xcopy static\\ dist\\static /E /I && xcopy upload\\ dist\\upload /E /I",
    "ts": "ts-node",
    "start:dev": "nodemon"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.3",
    "@types/bcrypt": "^5.0.0",
    "@types/convict": "6.1.1",
    "@types/convict-format-with-validator": "6.0.2",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "2.8.5",
    "@types/express": "4.17.14",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/mime-types": "2.1.1",
    "@types/ms": "^0.7.32",
    "@types/multer": "1.4.7",
    "@types/node": "18.16.3",
    "@types/nodemailer": "^6.4.13",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "eslint": "8.39.0",
    "eslint-config-htmlacademy": "9.1.1",
    "nodemon": "^3.0.1",
    "pino-pretty": "9.1.1",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "@types/mongoose": "^5.11.97"
  },
  "type": "module",
  "engines": {
    "node": "^18.0.0",
    "npm": ">=8"
  },
  "dependencies": {
    "@typegoose/typegoose": "^11.7.1",
    "amqplib": "^0.10.3",
    "bcrypt": "^5.1.1",
    "chalk": "5.2.0",
    "class-transformer": "0.5.1",
    "class-validator": "^0.14.0",
    "convict": "^6.2.4",
    "convict-format-with-validator": "6.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "2.8.5",
    "dayjs": "1.11.7",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-async-handler": "1.2.0",
    "got": "12.6.0",
    "http-status-codes": "2.2.0",
    "inversify": "6.0.1",
    "jose": "4.11.1",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "mongoose": "^7.5.0",
    "ms": "^2.1.3",
    "multer": "1.4.5-lts.1",
    "nanoid": "4.0.0",
    "nodemailer": "^6.9.7",
    "pino": "8.7.0",
    "reflect-metadata": "0.1.13",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1"
  }
}
